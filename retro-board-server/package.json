  "private": true,
  "scripts": {
    "build": "rm -rf dist && ../node_modules/typescript/bin/tsc --build",
    "start": "nodemon --exec ts-node ./src/index.ts",
    "create-migration": "ts-node ../node_modules/typeorm/cli.js migration:generate -n ",
    "migrate": "ts-node ./src/init.ts && ts-node ../node_modules/typeorm/cli.js migration:run",
    "revert": "ts-node ../node_modules/typeorm/cli.js migration:revert"
  },
  "dependencies": {
    "@types/dotenv": "^6.1.1",
    "@types/express": "^4.17.1",
    "@types/lodash": "^4.14.139",
    "@types/md5": "^2.1.33",
    "@types/nedb": "^1.8.9",
    "@types/node": "^12.7.5",
    "@types/socket.io": "^2.1.2",
    "@types/uuid": "^3.4.5",
    "chalk": "^2.4.2",
    "cross-env": "6.0.0",
    "date-fns": "^2.2.1",
    "dotenv": "^8.1.0",
    "express": "^4.17.1",
    "lodash": "^4.17.15",
    "md5": "^2.2.1",
    "moment": "^2.24.0",
    "nedb": "^1.8.0",
    "nodemon": "^1.19.2",
    "pg": "^7.12.1",
    "retro-board-common": "*",
    "socket.io": "^2.3.0",
    "ts-node": "^8.4.1",
    "typeorm": "^0.2.19",
    "uuid": "^3.3.3"
  },
  "nodemonConfig": {
    "watch": "./src/",
    "ext": "ts,tsx,json"
  }
}